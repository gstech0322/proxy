{
   "background": {
      "scripts": [
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icon.png"
      },
      "default_title": "Genie"
   },
   "description": "Chat gpt",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "60",
   "name": "Genie",
   "permissions": [
      "tabs",
      "activeTab",
      "webNavigation",
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "storage",
      "contextMenus",
      "<all_urls>",
      "bookmarks",
      "notifications",
      "proxy",
      "idle"
   ],
   "short_name": "XBlocker",
   "version": "1.0.4"
}